{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "title": "Custom Meta Source",
  "description": "A representation of a custom meta source in system",
  "allOf": [
    {
      "$ref": "./meta_source.shared.json"
    }
  ],
  "properties": {
    "sourceType": {
      "type": "string",
      "const": "custom"
    }
  },
  "required": [
    "sourceType"
  ],
  "unevaluatedProperties": false
}