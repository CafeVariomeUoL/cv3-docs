        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <build-group name="cv3-docs">
        <instance instance-id="ug" alias="User Guide" path="/user"/>
        <instance instance-id="ag" alias="Admin Guide" path="/admin"/>
        <instance instance-id="dg" alias="Developer Guide" path="/developer"/>
        <instance instance-id="bts" alias="Biomedical Term Service" path="/bts"/>
    </build-group>
</build-groups>
